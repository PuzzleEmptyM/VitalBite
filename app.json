{
    "expo": {
      "name": "VitalBite",
      "slug": "vitalbite",
      "entryPoint": "../AppEntry.tsx",
      "platforms": ["ios", "android", "web"],
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "enabled": true,
        "checkAutomatically": "ON_ERROR_RECOVERY"
      },
      "assetBundlePatterns": [
        "**/*"
      ]
    }
  }
  